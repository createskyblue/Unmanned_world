                     绘图
  =========================================================*/
void draw()
{
  arduboy.clear();
  if (millis() > 3191 && millis() < 10000 && millis() >= 100 + Timer[0]) {
    Timer[0] = millis();
    PPTY++;
  }
  if (millis() < 8682) {
    arduboy.drawBitmap(16, 16 - PPTY, title, 90, 25, 1);
    drawText(32, 46 - PPTY, mes0, sizeof(mes0));
    drawText(48, 57 - PPTY, mes1, sizeof(mes1));
  }
  if (millis() > 8682 && millis() < 14486) arduboy.drawSlowXYBitmap(0, 0, cg1, 128, 64, 1); //cg1
  if (millis() > 8682  && millis() < 10911 ) Blur(0, 0, 128, 64, 0);
  if (millis() > 8682  && millis() < 12966 ) Blur(0, 0, 128, 64, 1);
  if (millis() > 16639  && millis() < 24294 ) drawText(8, 9, mes2, sizeof(mes2));
  if (millis() > 18423  && millis() < 24294 ) drawText(8, 24, mes3, sizeof(mes3));
  if (millis() > 21321  && millis() < 24294 ) drawText(8, 40, mes4, sizeof(mes4));
  if (millis() > 24295  && millis() < 36100 ) arduboy.drawSlowXYBitmap(0, 0, cg2, 128, 64, 1); //cg2
